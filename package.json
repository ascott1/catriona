{
  "name": "catriona",
  "version": "0.1.0",
  "description": "A Jekyll boilerplate",
  "author": "Adam Scott",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "browser-sync": "^2.11.1",
    "node-minify": "^1.3.7",
    "parallelshell": "^2.0.0",
    "postcss": "^5.0.14",
    "postcss-browser-reporter": "^0.4.0",
    "postcss-cli": "^2.5.0",
    "postcss-conditionals": "^2.0.1",
    "postcss-css-variables": "^0.5.1",
    "postcss-custom-media": "^5.0.0",
    "postcss-import": "^8.0.2",
    "precss": "^1.4.0",
    "rimraf": "^2.5.1",
    "watch": "^0.17.1"
  },
  "scripts": {
    "clean": "rm -r _site/**",
    "build:css": "postcss -c css.json --watch --verbose _/css/*.css",
    "sync": "browser-sync start --proxy localhost:4000 --files _site/**",
    "start": "parallelshell 'jekyll serve' 'npm run build:css'"
  }
}
